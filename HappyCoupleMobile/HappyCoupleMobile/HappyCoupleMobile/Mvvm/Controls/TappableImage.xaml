<?xml version="1.0" encoding="UTF-8"?>
<Image xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
             x:Class="HappyCoupleMobile.Mvvm.Controls.TappableImage" x:Name="TappableImage">
    <Image.GestureRecognizers>
        <TapGestureRecognizer Command="{Binding Source={x:Reference TappableImage}, Path=TransitionCommand}" />
    </Image.GestureRecognizers>
</Image>
